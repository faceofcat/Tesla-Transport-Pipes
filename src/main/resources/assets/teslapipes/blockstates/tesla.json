{
  "multipart": [
    {
      "apply": {
        "model": "teslapipes:pipe_tesla"
      }
    },
    {
      "when": { "north": "true" },
      "apply": {
        "model": "teslapipes:pipe_tesla-connector"
      }
    },
    {
      "when": { "east": "true" },
      "apply": {
        "model": "teslapipes:pipe_tesla-connector", "y": 90
      }
    },
    {
      "when": { "south": "true" },
      "apply": {
        "model": "teslapipes:pipe_tesla-connector", "y": 180
      }
    },
    {
      "when": { "west": "true" },
      "apply": {
        "model": "teslapipes:pipe_tesla-connector", "y": 270
      }
    },
    {
      "when": { "down": "true" },
      "apply": {
        "model": "teslapipes:pipe_tesla-connector", "x": 90
      }
    },
    {
      "when": { "up": "true" },
      "apply": {
        "model": "teslapipes:pipe_tesla-connector", "x": 270
      }
    }
  ]
}